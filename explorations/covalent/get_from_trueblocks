#!/usr/bin/env bash

chifra export --no_header --cache $1 >store/raw/trueblocks/$1.txt

cat store/raw/trueblocks/$1.txt | \
    cut -f1,2,10 | \
    awk -F'\t' '{print $3,$1,$2}' OFS='\t' | \
    sort >store/processed/trueblocks/$1.txt

./cleanup processed/trueblocks $1 >x
cat x >store/processed/trueblocks/$1.txt

gzip -f store/raw/trueblocks/$1.txt

cat store/processed/trueblocks/$1.txt | sort -k 2 -n | head -1
cat store/processed/trueblocks/$1.txt | sort -k 2 -n | tail -1
